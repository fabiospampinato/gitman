{
  "name": "@fabiospampinato/gitman",
  "displayName": "gitman",
  "description": "A simple yet powerful opinionated tool for managing GitHub repositories.",
  "version": "1.2.1",
  "bin": "dist/src/bin.js",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/gitman/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/gitman.git"
  },
  "keywords": [
    "git",
    "github",
    "man",
    "manager",
    "automation",
    "multiple",
    "bulk",
    "shell",
    "cli",
    "terminal",
    "app"
  ],
  "dependencies": {
    "cli-truncate": "^2.1.0",
    "cli-width": "^3.0.0",
    "micromatch": "^4.0.4",
    "simple-get": "^4.0.0",
    "specialist": "^0.4.1"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.2",
    "@types/node": "^16.11.7",
    "typescript": "^4.4.4",
    "typescript-transform-export-interop": "^1.0.4"
  }
